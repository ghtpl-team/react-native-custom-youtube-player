{
  "name": "react-native-custom-youtube-player",
  "version": "1.0.0",
  "description": "React Native wrapper for custom YouTube player with native Android implementation",
  "engines": {
    "node": ">=18.18.0"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "react-native-youtube-viewpager.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!android/.cxx",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn"
  },
  "keywords": [
    "react-native",
    "youtube",
    "video",
    "player",
    "android",
    "custom"
  ],
  "repository": "https://github.com/ghtpl-team/react-native-custom-youtube-player",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghtpl-team/react-native-custom-youtube-player/issues"
  },
  "homepage": "https://github.com/ghtpl-team/react-native-custom-youtube-player#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-config": {
    "android": {
      "sourceDir": "../node_modules/react-native-custom-youtube-player/android/",
      "packageImportPath": "import com.youtubevp.YoutubePlayerPackage;"
    }
  }
}